{
  "name": "webpack-react-styled-tpl",
  "version": "1.0.0",
  "private": true,
  "license": "ISC",
  "scripts": {
    "serven": "webpack serve --mode development",
    "start": "webpack serve --config=webpack.config.js --mode=development",
    "start:http": "webpack serve --host   127.0.0.1 --config=webpack.config.js --mode=development",
    "buildn": "webpack --mode production",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "eslint": "eslint src",
    "eslint:fix": "npm run eslint -- --fix",
    "precommit": "lint-staged --debug"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.8",
    "styled-components": "^6.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/register": "^7.22.15",
    "@svgr/webpack": "^8.1.0",
    "@types/cors": "^2.8.15",
    "@types/dotenv-webpack": "^7.0.6",
    "@types/html-webpack-plugin": "^3.2.8",
    "@types/node": "^20.8.10",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.11",
    "@types/styled-components": "^5.1.29",
    "@types/uuid": "^9.0.6",
    "@types/webpack": "^5.28.4",
    "@types/webpack-assets-manifest": "^5.1.2",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "babel-plugin-styled-components": "^2.1.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.3",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "react-svg-loader": "^3.0.3",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "typescript-plugin-styled-components": "^3.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "babel-plugin-styled-components",
      "@babel/plugin-transform-runtime",
      "react-css-modules"
    ]
  },
  "lint-staged": {
    "*.{js, json}": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run eslint:fix",
      "prettier --write --ignore-unknown",
      "node_modules/.bin/eslint ./src --max-warnings=0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
